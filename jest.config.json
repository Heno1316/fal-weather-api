{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
        "<rootDir>"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "coverageThreshold": {
        "global": {
            "branches": 0,
            "functions": 0,
            "lines": 33,
            "statements": 40
        }
    },
    "testEnvironment": "node"
}